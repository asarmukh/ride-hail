{
    "rabbit_version": "3.12.0",
    "users": [
      {
        "name": "ridehail",
        "password": "ridehail_pass",
        "tags": "administrator"
      }
    ],
    "vhosts": [
      { "name": "/" }
    ],
    "permissions": [
      {
        "user": "ridehail",
        "vhost": "/",
        "configure": ".*",
        "write": ".*",
        "read": ".*"
      }
    ],
    "exchanges": [
      { "name": "ride_topic", "vhost": "/", "type": "topic", "durable": true, "auto_delete": false, "internal": false },
      { "name": "driver_topic", "vhost": "/", "type": "topic", "durable": true, "auto_delete": false, "internal": false },
      { "name": "location_fanout", "vhost": "/", "type": "fanout", "durable": true, "auto_delete": false, "internal": false }
    ],
    "queues": [
      { "name": "ride_requests", "vhost": "/", "durable": true, "auto_delete": false, "arguments": {} },
      { "name": "ride_status", "vhost": "/", "durable": true, "auto_delete": false, "arguments": {} },
      { "name": "driver_matching", "vhost": "/", "durable": true, "auto_delete": false, "arguments": {} },
      { "name": "driver_responses", "vhost": "/", "durable": true, "auto_delete": false, "arguments": {} },
      { "name": "driver_status", "vhost": "/", "durable": true, "auto_delete": false, "arguments": {} },
      { "name": "location_updates_ride", "vhost": "/", "durable": true, "auto_delete": false, "arguments": {} },
      { "name": "location_updates_driver", "vhost": "/", "durable": true, "auto_delete": false, "arguments": {} }
    ],
    "bindings": [
      {
        "source": "ride_topic",
        "vhost": "/",
        "destination": "ride_requests",
        "destination_type": "queue",
        "routing_key": "ride.request.*",
        "arguments": {}
      },
      {
        "source": "ride_topic",
        "vhost": "/",
        "destination": "ride_status",
        "destination_type": "queue",
        "routing_key": "ride.status.*",
        "arguments": {}
      },
      {
        "source": "ride_topic",
        "vhost": "/",
        "destination": "driver_matching",
        "destination_type": "queue",
        "routing_key": "ride.request.*",
        "arguments": {}
      },
      {
        "source": "driver_topic",
        "vhost": "/",
        "destination": "driver_responses",
        "destination_type": "queue",
        "routing_key": "driver.response.*",
        "arguments": {}
      },
      {
        "source": "driver_topic",
        "vhost": "/",
        "destination": "driver_status",
        "destination_type": "queue",
        "routing_key": "driver.status.*",
        "arguments": {}
      },
      {
        "source": "location_fanout",
        "vhost": "/",
        "destination": "location_updates_ride",
        "destination_type": "queue",
        "arguments": {}
      },
      {
        "source": "location_fanout",
        "vhost": "/",
        "destination": "location_updates_driver",
        "destination_type": "queue",
        "arguments": {}
      }
    ],
    "parameters": [],
    "policies": [],
    "global_parameters": [
      { "name": "cluster_name", "value": "rabbit@ridehail" }
    ]
